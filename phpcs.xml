<?xml version="1.0"?>
<ruleset name="Castanet">
  <arg name="colors" />
  <arg name="tab-width" value="4" />
  <arg name="extensions" value="php" />
  <arg name="encoding" value="utf-8" />
  <arg name="warning-severity" value="0" />
  <arg value="p" /><!-- show progress -->

  <file>./Castanet.php</file>
  <file>./Castanet</file>

  <rule ref="PEAR" />
</ruleset>
